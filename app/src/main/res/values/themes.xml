<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.2200581322004525" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/DarkGrey</item>
        <item name="colorPrimaryVariant">@color/Grey</item>
        <item name="background">@color/bgColor</item>
        <!-- Status bar color. -->
        <item name="colorSecondary">@color/lightPurple</item>
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>
    <style name="ToolBarTheme" parent="Theme.2200581322004525">
        <item name="android:background">@color/DarkGrey</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:textColorSecondary">@color/white</item>
    </style>

    <style name="RoundCorner" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="color">@color/white</item>
    </style>
</resources>
